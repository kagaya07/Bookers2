<h2>Users</h2>
	<table>
		<thead>
			<tr>
				<th>image</th>
				<th>name</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<% @user.each do |user| %>
					<tb>
						<%= attachment_image_tag user, :profile_image, :fill, 80, 80, format: 'jpeg', class: "img-circle pull-left profile-img", fallback: "no_image.jpg" %>
					</tb>
					<tb>
						<%= link_to user_path(user.id) do %>
						<%= user.name %>
						<% end %>
					</tb>
				<% end %>
			</tr>
		</tbody>
	</table>
</div>
